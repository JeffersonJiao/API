<div class="container">
    <h1>Top Scorer</h1>
    <table class="table table-dark table-striped">
        <thead>
            <tr>
            <th>Name</th>
            <th>Score</th>
            </tr>
        </thead>
        <% if(leaders) { %>
            <% for(var i = 0 ; i < leaders.length ; i ++) { %>
            <tr>
                <td><%=leaders[i].username%></td>
                <td><%=leaders[i].highestScore%></td>
            </tr>
            <% } %>
        <% } %>
    </table>

</div>
 